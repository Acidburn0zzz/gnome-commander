msgstr ""

#: ../src/gnome-cmd-tags.c:119 ../src/gnome-cmd-tags.c:434
msgid "CFA Pattern"
msgstr "CFA-malli"

#: ../src/gnome-cmd-tags.c:119
msgid ""
