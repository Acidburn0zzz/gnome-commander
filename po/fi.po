msgid "Battery Level"
msgstr "Akun taso"

#: ../src/gnome-cmd-tags.c:109
msgid "Bits per Sample"
msgstr "Bittejä näytteessä"

#: ../src/gnome-cmd-tags.c:109
msgid ""
